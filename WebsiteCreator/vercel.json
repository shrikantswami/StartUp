{
  "version": 2,
  "builds": [
    {
      "src": "WebsiteCreator/wsgi.py",
      "use": "@vercel/python",
      "config": { "maxLambdaSize": "15mb" , "runtime": "python3.12"}
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "WebsiteCreator/wsgi.py"
    }
  ],
  "env": {
    "DEBUG": "false",
    "ALLOWED_HOSTS": ".vercel.app"
  }
}
